    "build": "tsup src --out-dir build",
    "lint": "eslint --ext .js,.ts . --fix",
    "__________________ tests __________________": "",
    "test:e2e:setup": "vitest run --dir src/http",
    "test:e2e:run": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:unit:run": "vitest run --dir src/use-cases",
    "test:unit:watch": "vitest --dir src/use-cases",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "______________ do not use ______________": "",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:e2e:setup": "run-s test:create-prisma-environment test:install-prisma-environment"
  },
